{
  "rewrites": [
    { "source": "/", "destination": "/home/index.html" },
    { "source": "/home", "destination": "/home/index.html" },
    { "source": "/home/", "destination": "/home/index.html" },

    { "source": "/myfirsresume", "destination": "/myfirsresume/index.html" },
    { "source": "/myfirsresume/", "destination": "/myfirsresume/index.html" },

    { "source": "/u/:slug", "destination": "/u/index.html?slug=:slug" },
    { "source": "/app", "destination": "/app/index.html" },
    { "source": "/app/", "destination": "/app/index.html" }
  ]
}

